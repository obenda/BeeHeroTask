{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "strictNullChecks": true
   },
   "include": ["src/**/*.ts", "src/db/seeding.ts", "src/db/dataSource.ts"]
}